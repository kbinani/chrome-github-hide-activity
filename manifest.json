{
	"name": "GitHub hide activity",
	"version": "0.0.0",
	"manifest_version": 2,
	"description": "GitHub hide activity",
	"permissions": [
		"storage"
	],
	"options_page": "options.html",
	"content_scripts": [
		{
			"matches": [
				"https://github.com/*"
			],
			"js": ["jquery-3.3.1.min.js", "main.js"]
		}
	]
}
